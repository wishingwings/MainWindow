<?xml version="1.0" encoding="UTF-8"?>
<Paras>
    <!--工程参数-->
    <!--卫星信息-->
    <!--选择卫星-->
    <ChooseSAT>
        <value>newvalue1</value>
        <value>newvalue1</value>
    </ChooseSAT>
    <!--星载光谱响应函数-->
    <SpectralResponseFunction>
        <value>3x+4y</value>
        <value>8x+9u</value>
    </SpectralResponseFunction>
    <!--卫星信息 END-->
    <!--机载信息-->
    <!--选择相机-->
    <ChooseCamera>
        <value>Camera_1</value>
        <value>Camera_2</value>
    </ChooseCamera>
    <!--相机信息-->
    <CameraParas>
        <!--焦距-->
        <focus>20</focus>
